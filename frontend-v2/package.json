{
  "name": "frontend-v2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "dev:mock": "VITE_ENABLE_MOCKS=true vite --port 3000",
    "build": "vite build && tsc",
    "prodbuild": "vite build && tsc",
    "serve": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:full": "playwright test --config playwright.config.full.ts",
    "test:clean-screenshots": "find tests -path '*/__screenshots__/*' -type f -delete",
    "lint": "eslint .",
    "format": "prettier --write .",
    "check": "prettier --check . && eslint . --max-warnings 0",
    "fix": "prettier --write . && eslint --fix .",
    "validate": "npm run fix && npm run test:run && npm run build"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@dagrejs/dagre": "^1.1.8",
    "@fontsource-variable/ibm-plex-sans": "^5.2.8",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-router": "^1.147.0",
    "@tanstack/router-plugin": "^1.147.0",
    "@xyflow/react": "^12.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "i18next": "^25.7.4",
    "lucide-react": "^0.563.0",
    "lz-string": "^1.5.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-globe.gl": "^2.37.0",
    "react-i18next": "^16.5.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.5",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tanstack/eslint-config": "^0.3.4",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/react-devtools": "^0.9.2",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-router-devtools": "^1.149.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^25.0.5",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/browser-playwright": "^4.0.17",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "eslint-plugin-license-header": "^0.8.0",
    "i18next-cli": "^1.34.1",
    "jsdom": "^28.0.0",
    "msw": "^2.12.7",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "shadcn": "^3.6.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.16",
    "vitest-browser-react": "^2.0.2",
    "web-vitals": "^5.1.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
